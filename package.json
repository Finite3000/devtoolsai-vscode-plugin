{
  "name": "dev-tools-ai",
  "displayName": "dev tools ai",
  "description": "Integrate dev tools ai into your IDE experience where it will learns from your tests, so you don't have to update them.",
  "icon": "resources/logo.png",
  "repository": {
	  "type": "git",
	  "url": "https://github.com/kirksl/devtoolsai-vscode-plugin"
  },
  "version": "0.0.1",
  "license": "MIT",
  "author": {
	"name": "dev tools ai"
  },
  "publisher": "dev-tools-ai",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
	"Programming Languages",
	"Machine Learning",
	"Testing",
    "Other"
  ],
  "keywords": [
	"devtools",
	"devtools.ai",
	"smartdriver",
	"ai",
	"ml",
	"artificial intelligence",
	"machine learning",
	"testing",
	"locator",
	"selector",
	"selenium",
	"webdriver",
	"appium",
	"cypress",
	"playwright"
  ],
  "activationEvents": [
	"onStartupFinished"
  ],
  "main": "./out/extension.js",
  "capabilities": {
	"untrustedWorkspaces": {
		"supported": true
	}
},
  "contributes": {
	"configuration": {
		"title": "dev tools ai",
		"properties": {
			"devtoolsai.setup.key": {
				"type": "string",
				"default": "",
				"description": "Your dev tools ai access key",
				"scope":"resource"
			}
		}
	},
    "commands": [
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2"
  }
}
